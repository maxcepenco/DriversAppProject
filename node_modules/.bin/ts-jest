#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/macs/Локальный диск/0_JS_BACKEND/Progect/NodeJS/project-start-lesson/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.0_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_f87850caec269b3ecfa3c3ba2685b0f4/node_modules/ts-jest/node_modules:/media/macs/Локальный диск/0_JS_BACKEND/Progect/NodeJS/project-start-lesson/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.0_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_f87850caec269b3ecfa3c3ba2685b0f4/node_modules:/media/macs/Локальный диск/0_JS_BACKEND/Progect/NodeJS/project-start-lesson/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/macs/Локальный диск/0_JS_BACKEND/Progect/NodeJS/project-start-lesson/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.0_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_f87850caec269b3ecfa3c3ba2685b0f4/node_modules/ts-jest/node_modules:/media/macs/Локальный диск/0_JS_BACKEND/Progect/NodeJS/project-start-lesson/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.0_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_f87850caec269b3ecfa3c3ba2685b0f4/node_modules:/media/macs/Локальный диск/0_JS_BACKEND/Progect/NodeJS/project-start-lesson/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-jest@29.4.1_@babel+core@7.28.0_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_f87850caec269b3ecfa3c3ba2685b0f4/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-jest@29.4.1_@babel+core@7.28.0_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_f87850caec269b3ecfa3c3ba2685b0f4/node_modules/ts-jest/cli.js" "$@"
fi
